{
    "name": "dude920228/php-cache",
    "description": "Simple cache implementation in php",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Dudás Krisztián",
            "email": "dude920228@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0",
        "ext-sockets": "*",
        "symfony/console": "^4.1",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
